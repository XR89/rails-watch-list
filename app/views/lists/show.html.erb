<h1>Lists#show</h1>

<%= @list.name %>
<ul>
  <% @bookmarks.each do |bookmark| %>
  <li><%= bookmark.movie.title %>-<%= link_to 'delete', bookmark_path(bookmark), method: :delete %></li>
  <%end %>
</ul>

<%= link_to 'Add a new bookmark', new_list_bookmark_path(@list) %>
<%= link_to 'Edit', edit_list_path(@list) %>
<%= link_to 'Back to lists', lists_path %>
